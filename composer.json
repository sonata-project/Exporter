{
    "name": "sonata-project/exporter",
    "description": "Lightweight Exporter library",
    "keywords": ["export", "csv", "xls", "data", "client"],
    "homepage": "https://github.com/sonata-project/Exporter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@gmail.com",
            "homepage": "https://sonata-project.org/"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.2",
        "doctrine/orm": "^2.2",
        "symfony/routing": "^2.8 || ^3.0",
        "symfony/property-access": "^2.8 || ^3.0",
        "symfony/phpunit-bridge": "^2.8 || ^3.0",
        "propel/propel1": "^1.6",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0"
    },
    "suggest": {
        "ext-curl": "*",
        "symfony/routing": "^2.8 || ^3.0",
        "symfony/property-access": "^2.8 || ^3.0",
        "propel/propel1": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Sonata\\Exporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\Exporter\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
